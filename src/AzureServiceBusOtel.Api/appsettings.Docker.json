{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Azure": "Warning",
      "Azure.Messaging.ServiceBus": "Information",
      "AzureServiceBusOtel.Api": "Information"
    }
  },
  "ServiceBus": {
    "ConnectionString": "",
    "OrdersQueueName": "orders-queue",
    "OrderProcessedQueueName": "order-processed-queue",
    "MaxConcurrentCalls": 5
  },
  "ApplicationInsights": {
    "ConnectionString": ""
  },
  "OpenTelemetry": {
    "OtlpEndpoint": "http://jaeger:4317",
    "EnableAzureMonitor": true,
    "EnableOtlp": true,
    "EnablePrometheus": true
  }
}

